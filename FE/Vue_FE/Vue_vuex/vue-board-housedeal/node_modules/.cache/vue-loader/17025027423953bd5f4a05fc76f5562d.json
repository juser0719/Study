b-th>글번호</b-th>\n              <b-th>제목</b-th>\n              <b-th>조회수</b-th>\n              <b-th>작성자</b-th>\n              <b-th>작성일</b-th>\n            </b-tr>\n          </b-thead>\n          <tbody>\n            <!-- 하위 component인 ListRow에 데이터 전달(props) -->\n            <qna-list-row\n              v-for=\"(article, index) in articles\"\n              :key=\"index\"\n              v-bind=\"article\"\n            />\n          </tbody>\n        </b-table-simple>\n      </b-col>\n      <!-- <b-col v-else class=\"text-center\">도서 목록이 없습니다.</b-col> -->\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport http from \"@/util/http-common\";\nimport QnaListRow from \"@/components/qna/child/QnAListRow\";\n\nexport default {\n  name: \"QnAList\",\n  components: {\n    QnaListRow,\n  },\n  data() {\n    return {\n      articles: [],\n    };\n  },\n  created() {\n    http.get(`/qna`).then(({ data }) => {\n      console.log(data);\n      this.articles = data;\n    });\n  },\n  methods: {\n    moveWrite() {\n      this.$router.push({ name: \"QnAWrite\" });\n    },\n  },\n};\n</script>\n\n<style scope>\n.tdClass {\n  width: 50px;\n  text-align: center;\n}\n.tdSubject {\n  width: 300px;\n  text-align: left;\n}\n</style>\n"]}]}