AAA,eAAA;AACA,SAAA,gBAAA;AACA,SAAA,OAAA;AACA,GArBA;AAsBA,EAAA,OAAA,gDACA,UAAA,CAAA,UAAA,EAAA,CACA,SADA,EAEA,UAFA,EAGA,SAHA,EAIA,cAJA,CAAA,CADA,GAOA,YAAA,CAAA,UAAA,EAAA,CACA,iBADA,EAEA,kBAFA,EAGA,iBAHA,EAIA,kBAJA,CAAA,CAPA;AAYA;AAEA,IAAA,SAdA,uBAcA;AACA,WAAA,gBAAA;AACA,WAAA,gBAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,UAAA,KAAA,QAAA,EAAA,KAAA,QAAA,CAAA,KAAA,QAAA;AACA,KAnBA;AAoBA,IAAA,QApBA,sBAoBA;AACA,WAAA,gBAAA;AACA,WAAA,eAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,SAAA,EAAA,KAAA,OAAA,CAAA,KAAA,SAAA;AACA,KAzBA;AA0BA,IAAA,SA1BA,uBA0BA;AACA,WAAA,gBAAA;AACA,UAAA,KAAA,QAAA,EAAA,KAAA,YAAA,CAAA,KAAA,QAAA,EAFA,CAEA;AACA,KA7BA;AA8BA,IAAA,WA9BA,yBA8BA,CAAA;AA9BA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <b-row class=\"mt-4 mb-4 text-center\">\r\n      <!-- <b-col class=\"sm-3\">\r\n      <b-form-input\r\n        v-model.trim=\"dongCode\"\r\n        placeholder=\"동코드 입력...(예 : 11110)\"\r\n        @keypress.enter=\"sendKeyword\"\r\n      ></b-form-input>\r\n    </b-col>\r\n    <b-col class=\"sm-3\" align=\"left\">\r\n      <b-button variant=\"outline-primary\" @click=\"sendKeyword\">검색</b-button>\r\n    </b-col> -->\r\n      <b-col class=\"sm-3\">\r\n        <b-form-select\r\n          v-model=\"sidoCode\"\r\n          :options=\"sidos\"\r\n          @change=\"gugunList\"\r\n        ></b-form-select>\r\n      </b-col>\r\n      <b-col class=\"sm-3\">\r\n        <b-form-select\r\n          v-model=\"gugunCode\"\r\n          :options=\"guguns\"\r\n          @change=\"dongList\"\r\n        ></b-form-select>\r\n      </b-col>\r\n      <b-col class=\"sm-3\">\r\n        <b-form-select\r\n          v-model=\"dongCode\"\r\n          :options=\"dongs\"\r\n          @change=\"searchApt\"\r\n        ></b-form-select>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions, mapMutations } from \"vuex\";\r\n\r\nconst houseStore = \"houseStore\";\r\n\r\nexport default {\r\n  name: \"HouseSearchBar\",\r\n  data() {\r\n    return {\r\n      sidoCode: null,\r\n      gugunCode: null,\r\n      dongCode: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(houseStore, [\"sidos\", \"guguns\", \"dongs\", \"houses\"]),\r\n    //mapState는 computed에 선언, state(data)와연결되어 값이 바뀌면 반응함\r\n    //mapgetters 도 여기에 선언함\r\n  },\r\n\r\n  created() {\r\n    this.CLEAR_SIDO_LIST();\r\n    this.CLEAR_GUGUN_LIST();\r\n    this.CLEAR_DONG_LIST();\r\n    this.CLEAR_HOUSE_LIST();\r\n    this.getSido();\r\n  },\r\n  methods: {\r\n    ...mapActions(houseStore, [\r\n      \"getSido\",\r\n      \"getGugun\",\r\n      \"getDong\",\r\n      \"getHouseList\",\r\n    ]),\r\n    ...mapMutations(houseStore, [\r\n      \"CLEAR_SIDO_LIST\",\r\n      \"CLEAR_GUGUN_LIST\",\r\n      \"CLEAR_DONG_LIST\",\r\n      \"CLEAR_HOUSE_LIST\",\r\n    ]), //action필요없이 바로 state에접근할때 사용하는애들\r\n\r\n    gugunList() {\r\n      this.CLEAR_HOUSE_LIST();\r\n      this.CLEAR_GUGUN_LIST();\r\n      this.gugunCode = null;\r\n      if (this.sidoCode) this.getGugun(this.sidoCode);\r\n    },\r\n    dongList() {\r\n      this.CLEAR_HOUSE_LIST();\r\n      this.CLEAR_DONG_LIST();\r\n      this.dongCode = null;\r\n      if (this.gugunCode) this.getDong(this.gugunCode);\r\n    },\r\n    searchApt() {\r\n      this.CLEAR_HOUSE_LIST();\r\n      if (this.dongCode) this.getHouseList(this.dongCode); // ->houses -> 마커를찍고싶다\r\n    },\r\n    sendKeyword() {},\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":"src/components/house"}]}